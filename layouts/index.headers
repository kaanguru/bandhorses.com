/*
  Strict-Transport-Security: max-age=31536000; includeSubDomains; preload
  X-Content-Type-Options: nosniff
  X-XSS-Protection: 0
  Content-Security-Policy: default-src 'self' 'unsafe-inline' ; manifest-src 'self'; connect-src 'self'; font-src 'self' data:  ; img-src 'self' data: ; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline' data: ;
  X-Frame-Options: SAMEORIGIN
  Referrer-Policy: same-origin
  Cache-Control: public, max-age=31536000
